{
  "name": "kit",
  "productName": "ScriptKit",
  "version": "0.0.0-development",
  "description": "The Script Kit App",
  "main": "./out/main/index.mjs",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "pnpm exec kit ./build.ts",
    "rebuild": "electron-rebuild",
    "clear-cache": "rimraf ./node_modules/.vite",
    "format:check": "biome check .",
    "format:fix": "biome check . --write --unsafe",
    "rebuild-node-pty": "electron-rebuild -f -o node-pty -m ./",
    "cp-vs-assets": "cp -R ./node_modules/monaco-editor/min/vs ./assets",
    "v": "PATH=~/.knode/bin:$PATH node --version",
    "write-node-version": "node ./scripts/write-node-version.js",
    "vite-dev": "./vite-dev.sh",
    "vite-prebuild": "electron-vite build",
    "vite-package": "electron-vite build && DEBUG_PROD=true electron-builder --mac --arm64 -c.mac.identity=null --publish never",
    "package:mac:arm64": "npm run vite-prebuild && pnpm exec kit ./build.ts mac arm64 never",
    "vite-build": "DEBUG_PROD=true electron-builder --mac --arm64 -c.mac.identity=null --publish never",
    "rimraf-kit-setup": "rimraf ~/.kit-setup ~/.kenv-setup ~/.knode-setup",
    "kit-setup": "KIT=~/.kit-setup KENV=~/.kenv-setup KNODE=~/.knode-setup electron-vite dev",
    "build-and-install:mac": "electron-vite build && pnpm exec kit ./build.ts mac arm64 never && rm -rf '/Applications/Script Kit.app' && mv 'release/mac-arm64/Script Kit.app' '/Applications/' && echo 'Script Kit.app installed. Remember to set KIT_APP_VERSION to the tagged app release version (e.g. 2.28.9) then: open /Applications/Script Kit.app'",
    "build-and-install:windows:x64": "electron-vite build && pnpm exec kit ./build.ts windows x64 never",
    "test": "vitest --config electron.vite.config.ts",
    "coverage": "vitest run --coverage --config electron.vite.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/script-kit/kitapp.git"
